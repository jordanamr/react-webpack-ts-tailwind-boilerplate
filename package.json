{
  "name": "react-webpack-ts-tailwind-boilerplate",
  "description": "Starter kit for React, TailwindCSS, Webpack (with Hot Module Replacement), TypeScript and Babel.",
  "version": "1.0.1",
  "author": "Jordan Rey",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jordanamr/react-webpack-ts-tailwind-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/jordanamr/react-webpack-ts-tailwind-boilerplate/issues"
  },
  "homepage": "https://github.com/jordanamr/react-webpack-ts-tailwind-boilerplate",
  "scripts": {
    "build": "yarn run clean-dist && cross-env NODE_ENV=production webpack -p --config=webpack/webpack.prod.config.ts",
    "clean-dist": "rimraf dist/*",
    "lint": "eslint './src/**/*.{ts,tsx}' --quiet",
    "start": "yarn run start:dev",
    "start:dev": "cross-env NODE_ENV=development webpack-dev-server --config=webpack/webpack.dev.config.ts"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@tailwindcss/custom-forms": "^0.2.1",
    "@types/node": "^14.14.14",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/webpack": "^4.41.25",
    "@types/webpack-dev-server": "^3.11.1",
    "@types/webpack-merge": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "autoprefixer": "^10.1.0",
    "awesome-typescript-loader": "5.2.1",
    "babel-loader": "^8.2.2",
    "babel-preset-minify": "^0.5.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.0",
    "image-webpack-loader": "^7.0.1",
    "imagemin-gifsicle": "^7.0.0",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.1",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^4.1.0",
    "prettier": "^2.2.1",
    "react-hot-loader": "^4.13.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.1.0",
    "source-map-loader": "^1.1.3",
    "style-loader": "^2.0.0",
    "stylelint": "^13.8.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "tailwindcss": "^2.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
